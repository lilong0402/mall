<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.lilong.mall.mapper.SysLogMapper">

    <insert id="insert" parameterType="String">
        insert into sys_log(user_name,content,behavior,create_time) values (#{username},#{content},#{behavior},NOW())
    </insert>
</mapper>